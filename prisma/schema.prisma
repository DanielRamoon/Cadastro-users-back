generator client {
  provider        = "prisma-client-js"
  // output       = "../generated/prisma"  
  // remova ou comente o output customizado
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  email   String @unique
  name    String
  age     String      
}
